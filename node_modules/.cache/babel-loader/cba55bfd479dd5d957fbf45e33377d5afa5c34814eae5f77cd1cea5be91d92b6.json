    })\r\n            .catch(error=>{\r\n                console.log(error);\r\n            }) \r\n            \r\n        }\r\n    },\r\n    created(){\r\n        this.getCategories();\r\n    },\r\n    \r\n};\r\n  </script>\r\n  \r\n  <style scoped>\r\n.admin-content {\r\n  padding: 20px;\r\n}\r\n\r\n.header-row {\r\n  align-items: center;\r\n  padding: 10px;\r\n  background-color: #2d2d2d;\r\n  border-radius: 8px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.header-title {\r\n  margin-left: 10px;\r\n  color: #f6d365;\r\n  font-weight: bold;\r\n  align-items: center;\r\n}\r\n\r\n.add-btn {\r\n  background-color: #f6d365;\r\n  box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -2px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.data-card {\r\n  overflow-x: auto;\r\n}\r\n\r\n.v-table {\r\n  background-color: #1e1e1e;\r\n  color: white;\r\n  border-radius: 4px;\r\n}\r\n\r\n.v-table thead th {\r\n  background-color: #f6d365;\r\n  color: #2d2d2d;\r\n  font-weight: bold;\r\n}\r\n\r\n.v-table tbody tr:hover {\r\n  background-color: #424242;\r\n}\r\n\r\n.v-btn {\r\n  margin: 0 4px;\r\n}\r\n\r\n.v-btn .mdi-icon {\r\n  color: white;\r\n}\r\n\r\n/* Responsive adjustments */\r\n@media (max-width: 600px) {\r\n  .header-row, .data-card {\r\n    padding: 5px;\r\n  }\r\n\r\n  .header-title, .v-table th, .v-table td {\r\n    font-size: 14px;\r\n  }\r\n\r\n  .v-btn {\r\n    min-width: 30px;\r\n    padding: 4px;\r\n  }\r\n\r\n  .v-icon {\r\n    font-size: 18px;\r\n  }\r\n}\r\n</style>\r\n  "],"mappings":"AAiFA,OAAAA,YAAA;AACA,OAAAC,KAAA;AACA,OAAAC,OAAA;AACA,OAAAC,QAAA;AACA;EACAC,UAAA;IAAAJ,YAAA;IAAAE,OAAA;IAAAC;EAAA;EACAE,IAAA;EACAC,KAAA;IACA;MACAC,UAAA;MACAC,SAAA;MACAC,UAAA;MACAC,WAAA;MACAC,YAAA;MACAC,UAAA;IACA;EACA;EACAC,OAAA;IACAC,cAAA;MACAb,KAAA,CAAAc,GAAA;MAAA,CACAC,IAAA,CAAAC,QAAA;QACA,KAAAV,UAAA,GAAAU,QAAA,CAAAX,IAAA;MACA,GACAY,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;MACA;IACA;IACAG,eAAA;MACArB,KAAA,CAAAsB,MAAA,sFAAAX,UAAA,EACAI,IAAA,CAAAC,QAAA;QACA,IAAAO,MAAA,QAAAjB,UAAA,CAAAkB,MA